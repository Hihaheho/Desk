*<
  @"files" [
    *<
      @"name" "main.ds",
      @"content" "
        'type add \\ *<@\"l\" 'number, @\"r\" 'number> -> @\"sum\" 'number;
        'handle 'begin
          $ ! \"a\" ~> 'number;
          <'number> ^add *<@\"l\" &'number, @\"r\" &'number>
        'end {
          'string ~> 'number =>
            ^add *<
              @\"l\" ! 1 ~> 'number,
              @\"r\" ! 2 ~> 'number,
            >
        }
      "
    >
  ],
  @"entrypoint" "main.ds",
  @"assertions" [
    @"RunSuccess" *<
      @"result" @"Number" @"Integer" 6
    >
  ]
>
