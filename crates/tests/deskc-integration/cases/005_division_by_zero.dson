*<
  @"files" [
    *<
      @"name" "main.ds",
      @"content" "
        'type add \\ *<@\"l\" 'number, @\"r\" 'number> -> @\"sum\" 'number;
        'type div \\ *<@\"dividend\" 'number, @\"divisor\" 'number> ->
          ! { @\"division by zero\" 'number ~> 'number } @\"quotient\" 'number;
        #1 'handle #2 <'number> ^ div *<@\"dividend\" 3, @\"divisor\" 0> '{
          @\"division by zero\" 'number ~> 'number =>
            <'number> ^add *<@\"l\" & @\"division by zero\" 'number, 1>
        '}
      "
    >
  ],
  @"assertions" *<
    @"typed" [*<
      @"file" "main.ds"
      @"typings" {
        1 => @"Number" *<>,
        2 => @"Effectful" *<
          @"ty" @"Number" *<>,
          @"effects" @"Effects" [
            *<
              @"input" @"Label" *<
                @"label" @"Literal" @"String" "division by zero",
                @"item" @"Number" *<>
              >,
              @"output" @"Number" *<>
            >
          ]
        >
      }
    >]
    @"runs" [
      *<
        @"entrypoint" @"File" "main.ds",
        @"result" @"Success" @"Number" @"Integer" 4
      >
    ]
  >
>
