{
  @files [
    {
      @name "main.ds",
      @content "
        $ #1 \\ #2 'a x ->
          <\\'number, 'number -> @sum 'number> 1, <#3 'a x> : 'a fun ~
        <'a fun> 2
      "
    }
  ],
  @entrypoint "main.ds",
  @assertions [
    @Typed [
      * 1, @Function {
        @parameters [
          @Number *
        ],
        @body @Label {
          @label "sum",
          @item @Number *
        }
      },
      * 2, @Number *,
      * 3, @Number *
    ],
    @RunSuccess {
      @result @Int 3
    }
  ]
}
