{
  @files [
    {
      @name "main.ds",
      @content "
      'type add \\ 'number, 'number -> @sum 'number ~
      'type sub \\ @minuend 'number, @subtrahend 'number -> 'number ~
      'type eq \\ 'number, 'number -> + @equal *, @unequal * ~
      $ \\ 'number ->
        + >eq >'number, 0 ~
          @equal * ->
            0,
          @unequal * ->
            + >eq >'number, 1 ~
              @equal * ->
                1,
              @unequal * ->
                ^>add
                  > \\'number -> 'number
                    >sub @minuend >'number, 1. ;
                  > \\'number -> 'number
                    >sub @minuend >'number, 2
                : 'number
      ~
      > \\'number -> 'number 7
      "
    }
  ],
  @entrypoint "main.ds",
  @assertions [
    @RunSuccess {
      @result @Int 13
    }
  ]
}
