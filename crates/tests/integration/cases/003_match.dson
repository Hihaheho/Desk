{
  @files [
    {
      @name "main.ds",
      @content "
        $ #1 @false *: 'a x ~
        #2
        + 'a x ~
          <@true *> -> 1,
          <@false *> -> 2.
      "
    }
  ],
  @entrypoint "main.ds",
  @assertions [
    @Typed [
      * 1, @Label {
        @label "false",
        @item @Product []
      },
      * 2, @Number *
    ],
    @RunSuccess {
      @result @Int 2
    }
  ]
}
