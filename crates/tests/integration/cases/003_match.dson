{
  @files [
    {
      @name "main.ds",
      @content "
        $ #1 @false *: x ~
        #2
        + <x> ~
          @true * -> 1,
          @false * -> 2.
      "
    }
  ],
  @entrypoint "main.ds",
  @assertions [
    @Typed [
      * 1, @Label {
        @label "false",
        @item @Product []
      },
      * 2, @Number *
    ],
    @RunSuccess {
      @result @Int 2
    }
  ]
}
